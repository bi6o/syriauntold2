{% extends 'base.html.twig' %}

{% block body %}
    <h1>Audio list</h1>

    <table>
        <thead>
            <tr>
                <th>Audiolink</th>
                <th>Cityid</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for audio in audios %}
            <tr>
                <td class="col-md-2">{{ audio.audioLink|raw }}</td>
                <td>{{ audio.cityId }}</td>
                <td><a href="{{ path('admin_audio_edit', { 'id': audio.id }) }}">edit</a></td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_audio_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
